# API Configuration
# Base URL for API requests - default development endpoint
VITE_APP_API_URL=http://localhost:3000

# API version identifier for request headers and routing
VITE_APP_API_VERSION=v1

# Authentication Configuration
# Google OAuth 2.0 client identifier - required for authentication
VITE_APP_GOOGLE_CLIENT_ID=your-google-client-id

# System Performance Configuration
# Request timeout in milliseconds (ensures <2s response time SLA)
VITE_APP_REQUEST_TIMEOUT=2000

# Maximum number of API request retry attempts for 99.9% uptime
VITE_APP_RETRY_ATTEMPTS=3

# Delay between retry attempts in milliseconds
VITE_APP_RETRY_DELAY=1000

# JWT token refresh interval in milliseconds (55 minutes)
VITE_APP_TOKEN_REFRESH_INTERVAL=3300000

# Environment Configuration
# Current environment identifier (development/staging/production)
VITE_APP_ENVIRONMENT=development

# Rate Limiting Configuration
# Maximum requests per minute for rate limiting
VITE_APP_RATE_LIMIT_REQUESTS=100

# Rate limit window in milliseconds (1 minute)
VITE_APP_RATE_LIMIT_WINDOW=60000

# Circuit Breaker Configuration
# Number of failures before circuit breaker opens
VITE_APP_CIRCUIT_BREAKER_THRESHOLD=5

# Circuit breaker reset timeout in milliseconds (30 seconds)
VITE_APP_CIRCUIT_BREAKER_RESET_TIMEOUT=30000

# Monitoring Configuration
# Enable/disable debug logging
VITE_APP_DEBUG_LOGGING=true

# Performance warning threshold percentage of timeout
VITE_APP_PERFORMANCE_WARNING_THRESHOLD=80

# Cache Configuration
# Cache duration in milliseconds (5 minutes)
VITE_APP_CACHE_DURATION=300000

# Maximum cache size in bytes (10MB)
VITE_APP_MAX_CACHE_SIZE=10485760