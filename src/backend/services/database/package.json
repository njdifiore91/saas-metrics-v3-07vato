{
  "name": "@startup-metrics/database",
  "version": "1.0.0",
  "private": true,
  "description": "Database service for Startup Metrics Benchmarking Platform",
  "main": "dist/client.js",
  "types": "dist/client.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/client.js",
    "dev": "ts-node-dev --respawn src/client.ts",
    "test": "jest --coverage --coverageThreshold=90",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "seed:dev": "ts-node src/seeds/development.ts",
    "seed:test": "ts-node src/seeds/test.ts",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@prisma/client": "5.2.0",
    "pino": "8.15.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.0",
    "express-rate-limit": "^6.9.0",
    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "2.4.3",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "cookie-parser": "^1.4.6",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "20.5.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@types/express": "^4.17.17",
    "eslint": "8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "29.6.2",
    "prettier": "3.0.2",
    "prisma": "5.2.0",
    "rimraf": "5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nock": "^13.3.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}