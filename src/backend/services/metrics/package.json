{
  "name": "@startup-metrics/metrics-service",
  "version": "1.0.0",
  "description": "Metrics processing and calculation service for the Startup Metrics Benchmarking Platform",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn src/app.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "proto:generate": "sh ../../scripts/generate-proto.sh",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "@prisma/client": "^5.2.0",
    "inversify": "^6.0.1",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "reflect-metadata": "^0.1.13",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^3.0.0",
    "circuit-breaker-js": "^0.0.1",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "supertest": "^6.3.3",
    "nock": "^13.3.2",
    "redis-mock": "^0.56.3",
    "ioredis-mock": "^8.9.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}